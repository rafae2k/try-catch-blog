{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "next",
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "react", "jsx-a11y", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "ignorePatterns": "*.config.ts",
  "rules": {
    "import/prefer-default-export": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
